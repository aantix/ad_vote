<!doctype html>
<html>
<head>
  <meta charset="utf-8"/>
  <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
  <title><%= content_for?(:title) ? yield(:title) : "Jim Jones, Rails Engineer" %></title>
  <meta name="description" content=""/>
  <meta name="author" content=""/>
  <%= stylesheet_link_tag "application", :media => "all" %>
  <%= javascript_include_tag "application", "jquery.pagescroller.lite" %>
  <%# javascript_include_tag "application", "jquery.scrollorama","jquery.scrollTo-1.4.2-min","jquery.easing.1.3","jquery.scrolldeck" %>
  <%= csrf_meta_tags %>
  <%= yield(:head) %>
</head>
<body>
<header class="navbar navbar-fixed-top">
  <nav class="navbar-inner">
    <div class="container">
      <%= render 'layouts/navigation' %>
    </div>
  </nav>
</header>
<div id="main" role="main">
  <div class="container">
    <div class="row-fluid">
      <div class="span12">
        <%= render 'layouts/messages' %>
        <%= yield %>
      </div>
    </div>
    <footer>
      <center>
        Images by <a href="http://www.sunnysideupphoto.com/">Sunny Side Up Photography</a>.<br/>
        Made with love in San Francisco, <%= Time.now.year %>.
      </center>
    </footer>
  </div>
  <!--! end of .container -->
</div>
<!--! end of #main -->
</body>
</html>